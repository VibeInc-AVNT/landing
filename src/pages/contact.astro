---
import Contact from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contact Vibe Cache: Request a Demo or Talk to Sales",
	description:
		"Get in touch with the Vibe Cache team. Request a demo, discuss enterprise solutions, or learn how semantic caching can optimize your AI applications.",
